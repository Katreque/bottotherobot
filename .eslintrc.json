{
    "env": {
        "browser": false,
        "commonjs": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-tabs": ["error", { "allowIndentationTabs": true }],
		"indent": ["error", "tab", {"SwitchCase": 1}],
		"no-console": "off",
		"no-await-in-loop": "off"
    }
}
